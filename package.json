{
  "name": "cat-robot",
  "version": "1.0.0",
  "description": "An awesome arduino and raspberry pi-powered robot for interacting with your cat!",
  "main": "index.js",
  "license": "MIT",
  "keywords": [
    "cat",
    "robot",
    "arduino",
    "raspberry",
    "pi"
  ],
  "engines": {
    "node": ">=11.8.0"
  },
  "scripts": {
    "start": "node index.js",
    "lint": "eslint '**/*.js'",
    "test": "nyc mocha",
    "prettier": "prettier --write './**/*.js'",
    "prettier:eslint": "eslint --print-config . | eslint-config-prettier-check",
    "docco": "docco {*.js,autoredeploy/*.js,scripts/*.js}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "./**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@tensorflow-models/mobilenet": "0.2.2",
    "@tensorflow/tfjs-node": "0.3.0",
    "dotenv": "6.2.0",
    "johnny-five": "1.0.0",
    "jpeg-js": "0.3.4",
    "keymirror": "0.1.1",
    "lodash": "4.17.11",
    "node-raspistill": "0.0.16",
    "pm2": "3.2.9",
    "pubnub": "4.21.7",
    "winston": "1.0.2"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "docco": "0.8.0",
    "eslint": "5.12.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.15.0",
    "eslint-plugin-prettier": "3.0.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "mocha": "5.2.0",
    "mockery": "2.1.0",
    "nyc": "13.2.0",
    "prettier": "1.16.4",
    "sinon": "7.2.3",
    "sinon-chai": "3.3.0"
  }
}
